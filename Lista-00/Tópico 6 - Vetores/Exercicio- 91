#include <stdio.h>

int main() {
    int tamanho = 50;
    int vetor[tamanho];

   
    for (int i = 0; i < tamanho; i++) {
        vetor[i] = (i + 5 * i) % (i + 1);
    }

    
    printf("Vetor preenchido com os valores especificados:\n");
    for (int i = 0; i < tamanho; i++) {
        printf("Vetor[%d] = %d\n", i, vetor[i]);
    }

    return 0;
}
