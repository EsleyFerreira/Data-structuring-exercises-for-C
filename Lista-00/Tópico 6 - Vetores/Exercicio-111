#include <stdio.h>

int main() {
    int valor[10];
    int tempo, i, j;

    printf("Digite dez valores num√©ros inteiros positivo:\n");

    
    for (i = 0; i < 10; i++) {
        printf("Digite o valor %d: ", i + 1);
        scanf("%d", &valor [i]);

    
        for (j = i; j > 0 && valor [j] < valor[j - 1]; j--) {
            
            tempo = valor[j];
            valor [j] = valor[j - 1];
            valor [j - 1] = tempo;
        }
    }

    printf("\nValores em ordem crescente:\n");

    
    for (i = 0; i < 10; i++) {
        printf("%d ", valor[i]);
    }

    printf("\n");

    return 0;
}

